; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Editor shaved prototype"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.941176, 0.945098, 0.882353, 1)
boot_splash/image="res://assets/splashscreen.png"
config/icon="res://icon.png"

[custom]

level_type=1
timeout=5.0
goal=0

[display]

window/size/viewport_width=1100
window/size/viewport_height=600
window/size/mode=2
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/editor-shaved/plugin.cfg")

[layer_names]

2d_physics/layer_1="avatar"
2d_physics/layer_2="walls"
2d_physics/layer_3="items"
2d_physics/layer_4="goal"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
